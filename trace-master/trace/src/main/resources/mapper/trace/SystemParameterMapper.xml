<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.trace.mapper.SystemParameterMapper">
    
    <resultMap type="SystemParameter" id="SystemParameterResult">
        <result property="systemParameterId"    column="system_parameter_id"    />
        <result property="systemParameterMeaning"    column="system_parameter_meaning"    />
        <result property="systemParameterValue"    column="system_parameter_value"    />
        <result property="systemParameterRemark"    column="system_parameter_remark"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectSystemParameterVo">
        select system_parameter_id, system_parameter_meaning, system_parameter_value, system_parameter_remark, create_time, update_time from trace_system_parameter
    </sql>

    <select id="selectSystemParameterList" parameterType="SystemParameter" resultMap="SystemParameterResult">
        <include refid="selectSystemParameterVo"/>
        <where>  
            <if test="systemParameterMeaning != null  and systemParameterMeaning != ''"> and system_parameter_meaning = #{systemParameterMeaning}</if>
            <if test="systemParameterValue != null  and systemParameterValue != ''"> and system_parameter_value = #{systemParameterValue}</if>
            <if test="systemParameterRemark != null  and systemParameterRemark != ''"> and system_parameter_remark = #{systemParameterRemark}</if>
        </where>
    </select>
    
    <select id="selectSystemParameterBySystemParameterId" parameterType="Long" resultMap="SystemParameterResult">
        <include refid="selectSystemParameterVo"/>
        where system_parameter_id = #{systemParameterId}
    </select>
        
    <insert id="insertSystemParameter" parameterType="SystemParameter" useGeneratedKeys="true" keyProperty="systemParameterId">
        insert into trace_system_parameter
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="systemParameterMeaning != null">system_parameter_meaning,</if>
            <if test="systemParameterValue != null">system_parameter_value,</if>
            <if test="systemParameterRemark != null">system_parameter_remark,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="systemParameterMeaning != null">#{systemParameterMeaning},</if>
            <if test="systemParameterValue != null">#{systemParameterValue},</if>
            <if test="systemParameterRemark != null">#{systemParameterRemark},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateSystemParameter" parameterType="SystemParameter">
        update trace_system_parameter
        <trim prefix="SET" suffixOverrides=",">
            <if test="systemParameterMeaning != null">system_parameter_meaning = #{systemParameterMeaning},</if>
            <if test="systemParameterValue != null">system_parameter_value = #{systemParameterValue},</if>
            <if test="systemParameterRemark != null">system_parameter_remark = #{systemParameterRemark},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where system_parameter_id = #{systemParameterId}
    </update>

    <delete id="deleteSystemParameterBySystemParameterId" parameterType="Long">
        delete from trace_system_parameter where system_parameter_id = #{systemParameterId}
    </delete>

    <delete id="deleteSystemParameterBySystemParameterIds" parameterType="String">
        delete from trace_system_parameter where system_parameter_id in 
        <foreach item="systemParameterId" collection="array" open="(" separator="," close=")">
            #{systemParameterId}
        </foreach>
    </delete>
</mapper>